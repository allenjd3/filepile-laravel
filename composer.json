{
    "name": "filepileio/filepile-laravel",
    "description": "Laravel Commands for FilePile.io",
    "type": "library",
    "require": {
        "symfony/console": "^3.3",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "FilePile",
            "email": "jeff@filepile.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "FilePile\\FilePileLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "FilePile\\FilePileLaravel\\Tests\\" : "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilePile\\FilePileLaravel\\Providers\\FilePileLaravelServiceProvider"
            ]
        }
    }
}
